{% extends 'base.html.twig' %}

{% block title %}UserImages index{% endblock %}

{% block body %}
    <h1>UserImages index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for user_image in user_images %}
            <tr>
                <td>{{ user_image.id }}</td>
                <td>{{ user_image.name }}</td>
                <td>
                    <a href="{{ path('app_user_images_show', {'id': user_image.id}) }}">show</a>
                    <a href="{{ path('app_user_images_edit', {'id': user_image.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_user_images_new') }}">Create new</a>
{% endblock %}
