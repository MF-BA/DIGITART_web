<form method="post" action="{{ path('app_participants_delete', {'idUser': app.user.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ app.user.id) }}">
    <button class="btn btn-danger"><i class="fa fa-trash-o"></i> Delete</button>
</form>
