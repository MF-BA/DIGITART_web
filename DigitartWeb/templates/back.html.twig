  .pagination {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 20px;
}

.pagination li {
  list-style: none;
  margin: 0 5px;
}

.pagination li a {
  text-decoration: none;
  color: #333;
  display: block;
  padding: 5px 10px;
  border: 1px solid #ccc;
  border-radius: 3px;
}

.pagination li.disabled a,
.pagination li.active a {
  color: #fff;
  background-color: #BD2A2E;
  border-color: #000000;
}

.pagination li.disabled a {
  cursor: not-allowed;
}

     
